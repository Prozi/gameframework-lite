{
  "name": "gameframework-lite",
  "version": "1.11.0",
  "description": "game framework for 2d html5 games with physics isomorphic",
  "main": "index.js",
  "browser": "es2015/index.js",
  "scripts": {
    "docs": "esdoc",
    "test": "node --harmony ./test",
    "lint": "eslint ./source --fix",
    "compile": "babel ./source --out-dir ./es2015"
  },
  "keywords": [
    "game framework",
    "game",
    "framework",
    "light",
    "lite",
    "es6",
    "es2015",
    "nodejs",
    "working",
    "ready"
  ],
  "editions": [
    {
      "description": "Source + ESNext + Import",
      "entry": "index.js",
      "directory": "source",
      "syntaxes": [
        "javascript",
        "esnext",
        "require",
        "arrows",
        "getset",
        "const",
        "let",
        "classes",
        "defaults",
        "spread",
        "rest",
        "destructuring",
        "template strings"
      ]
    },
    {
      "description": "Compiled + ES2015 + Require",
      "entry": "index.js",
      "directory": "es2015",
      "syntaxes": [
        "javascript",
        "es2015",
        "require"
      ]
    }
  ],
  "author": "Jacek Pietal <prozi85@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "editions": "^1.3.3",
    "fmath": "latest",
    "gameloop-compatible": "latest",
    "pixi.js": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.16.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Prozi/gameframework-lite.git"
  }
}
