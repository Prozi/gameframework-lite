{
  "name": "gameframework-lite",
  "version": "1.7.3",
  "description": "game framework for 2d html5 games with physics isomorphic",
  "main": "index.js",
  "scripts": {
    "docs": "esdoc",
    "test": "yarn compile && webpack && node test",
    "compile": "babel ./source --out-dir ./es2015"
  },
  "keywords": [
    "game framework",
    "game",
    "framework",
    "light",
    "lite",
    "es6",
    "es2015",
    "nodejs",
    "working",
    "ready"
  ],
  "editions": [
    {
      "description": "Source + ESNext + Require",
      "entry": "source/index.js",
      "syntaxes": [
        "javascript",
        "esnext",
        "import",
        "arrows",
        "classes",
        "const",
        "let",
        "destructuring"
      ]
    },
    {
      "description": "Compiled + ES5 + Require",
      "entry": "es2015/index.js",
      "syntaxes": [
        "javascript",
        "ES5",
        "require"
      ]
    }
  ],
  "author": "Jacek Pietal <prozi85@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "editions": "^1.3.3",
    "pixi.js": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "express": "^4.16.2",
    "fmath": "latest",
    "webpack": "^3.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Prozi/gameframework-lite.git"
  }
}
